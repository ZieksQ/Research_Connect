{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://inquira-backend-render.onrender.com/api/:path*"
    },
    {
      "source": "/user/:path*",
      "destination": "https://inquira-backend-render.onrender.com/user/:path*"
    },
    {
      "source": "/survey/:path*",
      "destination": "https://inquira-backend-render.onrender.com/survey/:path*"
    },
    {
      "source": "/oauth/:path*",
      "destination": "https://inquira-backend-render.onrender.com/oauth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
